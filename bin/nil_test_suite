#!/bin/bash

function test_devnull {
  for _ in {1..1000}; do
    echo foo > /dev/null 2>&1;
  done;
}

function test_file {
  for _ in {1..1000}; do
    echo bar > ~/.nil 2>&1;
  done;
}

function test_exit {
  for _ in {1..1000}; do
    echo baz | exit 0 2>&1;
  done;
}

function test_close_out {
  for _ in {1..1000}; do
    echo qux 1>&- 2>&-;
  done;
}

echo -ne "\ndevnull"
time test_devnull

echo -ne "\nfile"
time test_file

echo -ne "\nexit"
time test_exit

echo -ne "\n1>&- 2>&-"
time test_close_out

